generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Client {
  id        String      @id @default(uuid())
  name      String
  userId    String      
  createAt  DateTime    @default(now())
  updatedAt DateTime    @updatedAt
}
